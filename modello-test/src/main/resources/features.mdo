<?xml version="1.0"?>

<model xmlns="http://codehaus-plexus.github.io/MODELLO/1.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://codehaus-plexus.github.io/MODELLO/1.4.0 http://codehaus-plexus.github.io/modello/xsd/modello-1.4.0.xsd"
  xml.namespace="http://codehaus-plexus.github.io/FEATURES/${version}"
  xml.schemaLocation="http://codehaus-plexus.github.io/features-${version}.xsd">
  <id>features</id>
  <name>ModelloFeaturesTest</name>
  <description>A model to test Modello features with every plugin and point to plugin specific features.</description>

  <defaults>
    <!--
     see modello-core org.codehaus.modello.model.ModelDefault class
     available keys: checkDeprecation, package, java.util.List, java.util.Map, java.util.Properties, java.util.Set
    -->
    <default>
      <key>package</key>
      <value>org.codehaus.modello.test.features</value>
    </default>
    <default><!-- to ensure elements order when writing back features.xml -->
      <key>java.util.Properties</key>
      <value>new org.codehaus.modello.OrderedProperties()</value>
    </default>
  </defaults>

  <versionDefinition><!-- feature really supported by StAX plugin only, which checks version of the document when parsing it -->
    <type>field</type><!-- other value: namespace, only in StAX plugin -->
    <value>versionField</value>
  </versionDefinition>

  <classes>

    <class rootElement="true" xml.tagName="features-demo">
      <name>Features</name>
      <version>1.0.0+</version>
      <!--
        1.0.0: main features, supported by every plugin,
        1.5.0: features supported by java and xdoc plugin but causing problems with some other plugins
        2.0.0: features supported by java plugin only but causing problems with some other plugins including xdoc
      -->
      <fields>
        <field>
          <name>versionField</name>
          <version>1.0.0+</version>
          <description>A field described as model's versionDefinition field: its value contains the version of the model used.</description>
          <type>String</type>
        </field>
        <field>
          <name>comment</name>
          <version>1.0.0+</version>
          <type>String</type>
          <comment>Test of a comment.</comment>
        </field>
        <field>
          <name>description</name>
          <version>1.0.0+</version>
          <description>Test of a description.</description>
          <type>String</type>
        </field>
        <field>
          <name>required</name>
          <version>1.0.0+</version>
          <required>true</required>
          <description><![CDATA[&lt;field&gt;&lt;required&gt;true&lt;/required&gt;&lt;/field&gt;: Test of a required field.]]></description>
          <type>String</type>
        </field>
        <field>
          <name>identifier</name>
          <version>1.0.0+</version>
          <description><![CDATA[&lt;field&gt;&lt;identifier&gt;true&lt;/identifier&gt;&lt;/field&gt;: a field taking part in instance identifier.
            This field is used to generate <code>toString()</code>, <code>hashCode()</code> and <code>equals()</code>
            methods.]]></description>
          <identifier>true</identifier>
          <type>String</type>
        </field>
        <field>
          <name>identifierPart2</name>
          <version>1.0.0+</version>
          <description>Multiple fields can be participate in an identifier, either simple types or complex ones.</description>
          <identifier>true</identifier>
          <association>
            <type>Reference</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>simpleTypes</name>
          <version>1.0.0+</version>
          <association>
            <type>SimpleTypes</type>
          </association>
        </field>
        <field>
          <name>defaultValues</name>
          <version>1.0.0+</version>
          <association>
            <type>SimpleTypes</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>arrays</name>
          <version>1.0.0+</version>
          <association>
            <type>ArrayFeatures</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>associationFeatures</name>
          <version>1.0.0+</version>
          <description>Test of association features.</description>
          <association>
            <type>AssociationFeatures</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>javaFeatures</name>
          <version>1.0.0+</version>
          <association>
            <type>JavaFeatures</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>xmlFeatures</name>
          <version>1.0.0+</version>
          <association>
            <type>XmlFeatures</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>xdocFeatures</name>
          <version>1.0.0+</version>
          <association>
            <type>XdocFeatures</type>
          </association>
        </field>
        <field>
          <name>node</name>
          <version>1.0.0+</version>
          <association>
            <type>NodeItem</type>
          </association>
        </field>
        <field>
          <name>cloneable</name>
          <version>1.0.0+</version>
          <association>
            <type>Thing</type>
          </association>
        </field>
        <field xdoc.separator="blank">
          <name>file</name>
          <version>1.0.0+</version>
          <association>
            <type>File</type>
          </association>
        </field>
        <field>
          <name>action</name>
          <version>1.0.0+</version>
          <association>
            <type>Action</type>
          </association>
        </field>
        <!--field>
          <name>rootRecursion</name>
          <version>1.0.0+</version>
          <association>
            <type>Features</type>
          </association>
        </field-->
      </fields>
      <codeSegments>
        <codeSegment>
          <version>1.0.0+</version>
          <code>
            /* this text goes directly into generated code */
          </code>
        </codeSegment>
        <codeSegment>
          <version>1.0.0+</version>
          <code>
