<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd">
  <bannerLeft>
    <name>Modello</name>
    <href>http://codehaus-plexus.github.io/modello/</href>
  </bannerLeft>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.7</version>
  </skin>

  <edit>${project.scm.url}</edit>

  <custom>
    <fluidoSkin>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <googlePlusOne />
      <googleSearch>
        <sitesearch>http://codehaus-plexus.github.io/modello/</sitesearch>
      </googleSearch>
    </fluidoSkin>
  </custom>

  <publishDate format="yyyy-MM-dd" position="right" />
  <version position="right" />

  <body>
    <links>
      <item name="Codehaus Plexus" href="http://codehaus-plexus.github.io/"/>
      <item name="Apache Maven" href="https://maven.apache.org/"/>
    </links>

    <!-- Breadcrumbs that every project will inherit -->
    <breadcrumbs>
      <item name="Modello" href="http://codehaus-plexus.github.io/modello/index.html" />
    </breadcrumbs>

    <menu name="Documentation" inherit="bottom">
      <item name="Modello Model" href="modello.html" />
      <item name="Location Tracking" href="location-tracking.html" />
      <item name="Maven Plugin" href="modello-maven-plugin/" />
      <item name="Release Notes" href="release-notes.html"/>
    </menu>

    <menu ref="modules" />

    <!-- Menus that every project will inherit -->
    <menu ref="reports" inherit="bottom" />

  </body>
</project>
